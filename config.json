{
  "locals": {
    "url": "http://localhost:8080",
    "name": "Floating City",
    "owner": "Ian Wollman",
    "description": "Fish slappin around"
  },
  "plugins": [
    "./plugins/paginator.coffee",
    "wintersmith-handlebars",
    "wintersmith-tag"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 3,
    "template": "index.html"
  },
  "handlebars": {
    "helperDir": "helpers"
  }
}
